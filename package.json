{
  "name": "bgsu-template",
  "version": "2.0.0-b20",
  "description": "BGSU template javascript for third-party systems.",
  "repository": {
    "type": "git",
    "url": "https://github.com/BGSU-LITS/bgsu-template.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run version && webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "version": "npm run version:config.js && npm run version:path.txt",
    "version:config.js":
      "genversion -P config,version -vt ejs/config.js.ejs dist/config.js",
    "version:path.txt":
      "genversion -P config,version -vt ejs/path.txt.ejs dist/path.txt"
  },
  "config": {
    "publicPath": "https://lib.bgsu.edu/template/"
  },
  "browserslist": [
    "defaults",
    "not op_mini all",
    "not kaios <= 2.5"
  ],
  "dependencies": {
    "@accede-web/tablist": "^2.0.1",
    "accessible-toggle": "^1.3.1",
    "chart.js": "^4.2.1",
    "flatpickr": "^4.6.13",
    "flickity": "^3.0.0",
    "flickity-bg-lazyload": "^2.0.0",
    "patternomaly": "^1.3.2",
    "scut": "^2.0.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@eslint/js": "^9.8.0",
    "@stylistic/eslint-plugin": "^2.6.2",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.0",
    "browserslist": "^4.22.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.8.0",
    "eslint-plugin-array-func": "^5.0.2",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-sonarjs": "^3.0.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-webpack-plugin": "^4.0.0",
    "genversion": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-svg-data-uri": "^1.4.4",
    "postcss": "^8.4.21",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.0",
    "postcss-reporter": "^7.0.5",
    "pug": ">=3.0.3",
    "pug3-loader": "^2.4.3",
    "sass": "^1.59.3",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.8.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-webpack-plugin": "^5.0.0",
    "svgo": "^3.0.2",
    "svgo-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.76.1",
    "webpack-cli": "^6.0.0",
    "webpack-dev-server": "^5.0.0"
  },
  "engines": {
    "node": "^20.11.0"
  }
}
